<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ezen.myProject.repogitory.UserDAO">
<!-- ★ 파라미터타입은 안적어도 알아서 적용됨. resultType은 꼭 적어야함!!! -->
	<select id="getUser" resultType="com.ezen.myProject.domain.UserVO">
		select * from user where id=#{id}
	</select>
	<insert id="insertUser">
		insert into user(id,pw,name,age,email,home)
		values(#{id},#{pw},#{name},#{age},#{email},#{home})
	</insert>
	<select id="getUsers"></select>
</mapper>